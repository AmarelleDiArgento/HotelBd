<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

    <servlet>
        <servlet-name>ReservaServ</servlet-name>
        <servlet-class>Control.ReservaServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AsignaPerServ</servlet-name>
        <servlet-class>Control.AsignaPerServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PermisoServ</servlet-name>
        <servlet-class>Control.PermisoServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServ</servlet-name>
        <servlet-class>Control.UsuarioServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RolServ</servlet-name>
        <servlet-class>Control.RolServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HabitaServ</servlet-name>
        <servlet-class>Control.HabitaServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicioServ</servlet-name>
        <servlet-class>Control.ServicioServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsumoServ</servlet-name>
        <servlet-class>Control.ConsumoServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReservaServ</servlet-name>
        <url-pattern>/reservas.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AsignaPerServ</servlet-name>
        <url-pattern>/asignapers.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PermisoServ</servlet-name>
        <url-pattern>/permisos.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServ</servlet-name>
        <url-pattern>/usuarios.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RolServ</servlet-name>
        <url-pattern>/rols.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HabitaServ</servlet-name>
        <url-pattern>/habitas.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicioServ</servlet-name>
        <url-pattern>/servicios.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsumoServ</servlet-name>
        <url-pattern>/consumos.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
